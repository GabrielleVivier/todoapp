<div class="notice">
  <% if flash[:notice] %>
  <%= flash[:notice] %></p>
<% end %>
</div> <br>

<%= form_for(@task) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :priority %>
  <%= f.select :priority, "very low" => 1, "low" => 2, "medium" => 3, "high" => 4 %>
  <%= f.label :due_date %>
  <%= f.text_field :due_date %>
  <%= f.submit %>
<% end %>
